                    <li class="col-md-2 col-sm-3 col-xs-6"><div class="thumbnail app-block-element">
                        <a class="app-block-link" href="{{= d.url}}" target="_blank">
                            <div class="app-block-icon tc mt20" >
								<div class="mt5">预算：{{= d.t40.F03}}</div>
								<div class="mt5">投标人数：{{= d.countBid}}</div>
								<div class="mt5">企业评分：{{= d.avg}}</div>
							</div>
                            <div class="caption app-block-title">
                                <h5>{{= d.t40.F02}}</h5>
                            </div>
                        </a>
                        <div class="app-block-over">
                            <a href="{{= d.url}}" target="_blank">
                                <span class="app-block-title mt10">{{= d.t40.F02}}</span>
                                <span class="app-block-promo mt10">{{= d.t40.F13}}</span>
                            </a>
                            <span class="app-block-detail">
                                <a href="{{= d.url}}" class="btn btn-mini btn-success" target="_blank">详情</a>
                            </span>
                        </div>
                        </div>
                    </li>
					{{/if}}
					{{/each}}
					</script>
            </section>
        </div>
