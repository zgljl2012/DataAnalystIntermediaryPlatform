                    <h2>
                       		可投标项目
                    </h2>
                </div>
                <ul class="row list-unstyled index-recommends-list" id="projectTarget">
                	
                </ul>
                <script type="text/x-jquery-tmpl" id="projectTmpl">
					{{each(i, d) data}}
